{
    "name": "spotlab/safeguard",
    "type": "library",
    "description": "A simple application that parses through a config.yml file, to backup database (mysql, sqlite, pgsql, dblib) and to archive files in tar.gz",
    "keywords": ["dump","backup","mysql","PDO","file"],
    "homepage": "http://github.com/spotlab/safeguard",
    "license": "MIT",
    "authors": [
        {
            "name": "LÃ©o POIROUX",
            "email": "leo.poiroux@gmail.com"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "4.0.*"
    },
    "require": {
        "php": ">=5.3.0",
        "symfony/console": "2.4.*",
        "symfony/yaml": "2.4.*",
        "symfony/filesystem": "2.4.*",
        "symfony/finder": "2.4.*",
        "ifsnop/mysqldump-php": "1.5"
    },
    "autoload": {
        "psr-0": {
            "Spotlab\\Safeguard": ["src/"]
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "bin": [
        "bin/safeguard"
    ]
}
